<!DOCTYPE html>
<html>

<head>
    <title>
        <%= title %>
    </title>
</head>

<body>
    <nav class="navbar navbar-expand-lg bg-primary text-uppercase">
        <div class="container">
            <h1 class="text-white">
                <%= title %>
            </h1>
            <div class="collapse navbar-collapse" id="navbarResponsive">
                <ul class="navbar-nav ms-auto">
                    <li class="nav-item mx-0 mx-lg-1"><a class="nav-link py-3 px-0 px-lg-3
rounded text-white" href="/">Home</a></li>
                    <li class="nav-item mx-0 mx-lg-1"><a class="nav-link py-3 px-0 px-lg-3
rounded text-white" href="/createqr">CreateQr</a></li>
                    <li class="nav-item mx-0 mx-lg-1"><a class="nav-link py-3 px-0 px-lg-3
rounded text-white" href="/Intervenant">Intervenants</a></li>
                </ul>
            </div>
        </div>
    </nav>
    <header class="masthead text-center">
        <div class="container d-flex align-items-center flex-column">
            <p></p>
        </div>
    </header>
    <div class="container-fluid align-items-center">
        <h1 class="cover-heading">Table du personnel</h1>
        <div class="container bg-light text-dark">
            <table class="table table-striped">
                <thead>
                    <tr>
                        <th>Identit√©</th>
                        <th>Code</th>
                        <th>Poste</th>
                        <th>Mail</th>
                    </tr>
                </thead>
                <tbody>
                    <% Intervenants.forEach(function(entry) { %>
                        <tr>
                            <td>
                                <%= entry.identite %>
                            </td>
                            <td>
                                <%= entry.code %>
                            </td>
                            <td>
                                <%= entry.poste %>
                            </td>
                            <td>
                                <%= entry.mail %>
                            </td>
                        </tr>
                        <% }); %>
                </tbody>
            </table>
        </div>
    </div>
</body>

</html>